CASSANDRADB
-----------
CREATE TABLE IF NOT EXISTS scipi.oagkw(keyword text, count int, PRIMARY KEY(keyword));
CREATE TABLE IF NOT EXISTS scipi.oagfos(fos text, count int, PRIMARY KEY(fos));
CREATE TABLE IF NOT EXISTS scipi.yrwisedist(year text, count int, PRIMARY KEY(year));


CREATE TABLE IF NOT EXISTS scipi.oagpub(doi text, title text, publisher text, venue text, PRIMARY KEY(doi));
CREATE TABLE IF NOT EXISTS scipi.oagpub(id text, title text, venue text, year text, keywords set<text>, publisher text, PRIMARY KEY(id));

KAFKA 
-----
bin/kafka-topics.sh --create --bootstrap-server localhost:9092 --replication-factor 1 --partitions 1 --topic oag



